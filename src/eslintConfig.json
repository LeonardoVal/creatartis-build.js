{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "airbnb-base"
  ],
  "globals": {},
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "class-methods-use-this": 0,
    "no-underscore-dangle": 0,
    "object-curly-newline": 0
  }
}